# Supabase Configuration
# Получите эти значения из настроек вашего проекта Supabase
# https://app.supabase.com/project/YOUR_PROJECT/settings/api

# URL вашего Supabase проекта
REACT_APP_SUPABASE_URL=https://your-project.supabase.co

# Публичный анонимный ключ (безопасно использовать на клиенте)
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Опциональные настройки
# Порт для локальной разработки
PORT=3000

# Режим разработки (development/production)
REACT_APP_ENV=development
